<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="ManageCookiesTests" verbose="1">

    <parameter name="url" value="https://tvtropes.org/" />

    <test name="Manage Cookies Chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.arrowsmith.tvtropestests.ManageCookiesTest" />
