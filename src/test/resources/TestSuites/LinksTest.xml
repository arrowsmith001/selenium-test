<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="LinksTests" verbose="1">

    <parameter name="browser" value="chrome" />


    <test name="Test All Links on Home Page">

        <parameter name="url" value="https://tvtropes.org/" />

        <classes>
            <class name="com.arrowsmith.tvtropestests.LinksTest" />
        </classes>
    </test>


    <test name="Test All Links on Browse Tropes Page">

        <parameter name="url" value="https://tvtropes.org/pmwiki/pmwiki.php/Main/Tropes" />

        <classes>
            <class name="com.arrowsmith.tvtropestests.LinksTest" />
        </classes>
    </test>



</suite>
